{
  "name": "youversity",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "cross-env ADAPTER_TARGET=vercel vite build",
    "build:vercel": "cross-env ADAPTER_TARGET=vercel vite build",
    "build:replit": "cross-env ADAPTER_TARGET=replit vite build",
    "build:capacitor": "cross-env ADAPTER_TARGET=capacitor vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "start": "node build/index.js",
    "cap:sync": "npm run build:capacitor && npx cap sync",
    "cap:add:android": "npx cap add android",
    "cap:add:ios": "npx cap add ios",
    "cap:open:android": "npx cap open android",
    "cap:open:ios": "npx cap open ios",
    "cap:build:android": "npm run build:capacitor && npx cap sync && npx cap open android",
    "cap:build:ios": "npm run build:capacitor && npx cap sync && npx cap open ios"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.2.0",
    "@capacitor/core": "^7.2.0",
    "@sveltejs/adapter-node": "^5.2.11",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/adapter-vercel": "^5.7.1",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/nodemailer": "^6.4.17",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.24",
    "svelte": "^4.2.7",
    "svelte-check": "^3.4.3",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "type": "module",
  "dependencies": {
    "@capacitor-firebase/authentication": "^7.2.0",
    "@capacitor/android": "^7.2.0",
    "@capacitor/app": "^7.0.1",
    "@capacitor/ios": "^7.2.0",
    "axios": "^1.7.7",
    "date-fns": "^4.1.0",
    "firebase": "^11.2.0",
    "firebase-admin": "^11.11.0",
    "https-proxy-agent": "^7.0.6",
    "lucide-svelte": "^0.456.0",
    "nodemailer": "^6.10.0",
    "shepherd.js": "^11.2.0",
    "svelte-motion": "^0.12.2",
    "youversity": "file:"
  },
  "engines": {
    "node": "20.x"
  }
}
